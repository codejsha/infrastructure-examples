{
  "name": "postgresql-source",
  "config": {
    "name": "postgresql-source",
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "key.converter": "io.confluent.connect.avro.AvroConverter",
    "key.converter.schema.registry.url": "http://schemaregistry.confluent.svc.cluster.local:8081",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://schemaregistry.confluent.svc.cluster.local:8081",
    "database.hostname": "host.docker.internal",
    "database.port": "5432",
    "database.user": "debezium",
    "database.password": "dbz",
    "database.server.name": "database1",
    "database.serverTimezone": "Asia/Seoul",
    "database.tcpKeepAlive": "true",
    "database.dbname": "dvdrental",
    "schema.include.list": "public",
    "table.include.list": "public.*",
    "time.precision.mode": "adaptive",
    "decimal.handling.mode": "precise",
    "tombstones.on.delete": "true",
    "snapshot.mode": "initial",
    "max.queue.size": "20240",
    "max.batch.size": "10240",
    "poll.interval.ms": "1000",
    "sanitize.field.names": "false"
  }
}
