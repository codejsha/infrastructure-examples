# minikube

function minikube() { echo "+ minikube ${@}">&2; command minikube ${@}; }

alias mk="minikube"
alias mka="minikube addons"
alias mkaenable="minikube addons enable"
alias mkadisable="minikube addons disable"
alias mkals="minikube addons list"
alias mkcache="minikube cache"

alias mkconfig="minikube config"
alias mkconfigview="minikube config view"

alias mkdashboard="minikube dashboard"
alias mkdashboardurl="minikube dashboard --url"

alias mkdockerenv="minikube docker-env"
alias mkip="minikube ip"
alias mkk="minikube kubectl"
alias mklo="minikube logs"
alias mkmount="minikube mount"

alias mkno="minikube node"
alias mknoadd="minikube node add"
alias mknols="minikube node list"
alias mknostart="minikube node start"
alias mknostop="minikube node stop"

alias mkpause="minikube pause"
alias mkpodmanenv="minikube podman-env"
alias mkprofile="minikube profile"
alias mkrm="minikube delete"

alias mksvc="minikube service"
alias mksvcls="minikube service list"

alias mkssh="minikube ssh"
alias mksshhost="minikube ssh-host"
alias mksshkey="minikube ssh-key"

alias mkstart="minikube start"
alias mkstatus="minikube status"
alias mkstop="minikube stop"

alias mktunnel="minikube ktunnel"
alias mkunpause="minikube unpause"
alias mkupdatecheck="minikube update-check"
alias mkupdatecontext="minikube update-context"
alias mkversion="minikube version"
