# process

alias killhttpd="sudo pgrep httpd | xargs sudo kill -9"
alias killjava="sudo pgrep java | xargs sudo kill -9"

alias psef="sudo ps -ef | sudo grep "
alias psefhttpd="sudo ps -ef | sudo grep httpd"
alias psefjava="sudo ps -ef | sudo grep java"
alias psefjboss="sudo ps -ef | sudo grep jboss"
alias psefnginx="sudo ps -ef | sudo grep nginx"
alias pseftomcat="sudo ps -ef | sudo grep tomcat"
alias psefweblogic="sudo ps -ef | sudo grep weblogic"

alias pxhttpd="sudo pgrep --exact --list-full httpd | sudo grep httpd"
alias pxdockerd="sudo pgrep --exact --list-full dockerd | sudo grep dockerd"
alias pxdockerproxy="sudo pgrep --exact --list-full docker-proxy | sudo grep docker-proxy"
alias pxnginx="sudo pgrep --exact --list-full nginx | sudo grep nginx"
alias pxjava="sudo pgrep --exact --list-full java | sudo grep java"
alias pxjavajboss="sudo pgrep --exact --list-full java | sudo grep jboss"
alias pxjavatomcat="sudo pgrep --exact --list-full java | sudo grep tomcat"
alias pxjavaweblogic="sudo pgrep --exact --list-full java | sudo grep weblogic"
