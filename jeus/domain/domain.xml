                </session-config>
            </web-engine>
            <jms-engine>
                <service-config>
                    <name>internal</name>
                    <listener-name>jms-internal1</listener-name>
                    <client-limit>1000</client-limit>
                    <client-keepalive-timeout>20</client-keepalive-timeout>
                </service-config>
                <thread-pool>
                    <min>10</min>
                    <max>20</max>
                </thread-pool>
                <connection-factory>
                    <type>queue</type>
                    <name>QueueConnectionFactory</name>
                </connection-factory>
                <connection-factory>
                    <type>topic</type>
                    <name>TopicConnectionFactory</name>
                </connection-factory>
            </jms-engine>
            <class-ftp>true</class-ftp>
            <jms-resource>
                <destination>
                    <type>queue</type>
                    <name>ExamplesQueue</name>
                </destination>
                <destination>
                    <type>topic</type>
                    <name>ExamplesTopic</name>
                </destination>
            </jms-resource>
        </server>
    </servers>
    <deployed-applications>
        <deployed-application>
            <id>examples</id>
            <path>/home/tmax/jeus7/samples/examples.ear</path>
            <type>EAR</type>
            <targets>
                <server>
                    <name>server1</name>
                </server>
            </targets>
            <options>
                <classloading>ISOLATED</classloading>
                <fast-deploy>false</fast-deploy>
                <keep-generated>false</keep-generated>
                <shared>false</shared>
            </options>
        </deployed-application>
    </deployed-applications>
</domain>
