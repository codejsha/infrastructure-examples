<?xml version="1.0" encoding="utf-8"?>
<domain xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="7.0">
    <production-mode>true</production-mode>
    <id>154507926</id>
    <admin-server-name>adminServer</admin-server-name>
    <security-manager>
        <default-application-domain>SYSTEM_DOMAIN</default-application-domain>
        <security-domains>
            <security-domain>
                <name>SYSTEM_DOMAIN</name>
            </security-domain>
        </security-domains>
    </security-manager>
    <servers>
        <server>
            <name>adminServer</name>
            <node-name>2269097c89bc</node-name>
            <jvm-config>
                <jvm-option>-Xmx1024m -XX:MaxMetaspaceSize=128m</jvm-option>
            </jvm-config>
            <class-ftp>true</class-ftp>
            <listeners>
                <base>BASE</base>
                <listener>
                    <name>BASE</name>
                    <listen-port>9736</listen-port>
                    <listen-address>0.0.0.0</listen-address>
                </listener>
                <listener>
                    <name>http-server</name>
                    <listen-port>8808</listen-port>
                    <listen-address>0.0.0.0</listen-address>
                    <keep-alive-timeout>600000</keep-alive-timeout>
                </listener>
                <listener>
                    <name>jms-internal</name>
                    <listen-port>9941</listen-port>
                    <listen-address>0.0.0.0</listen-address>
                </listener>
            </listeners>
            <web-engine>
                <blocked-url-patterns>
                    <deny-last-space-character>true</deny-last-space-character>
                    <deny-null-character>true</deny-null-character>
                    <encoded-pattern>%23</encoded-pattern>
                    <encoded-pattern>%2e</encoded-pattern>
                    <encoded-pattern>%2f</encoded-pattern>
                    <encoded-pattern>%5c</encoded-pattern>
                    <encoded-pattern>%00</encoded-pattern>
                    <decoded-pattern>#</decoded-pattern>
                </blocked-url-patterns>
                <web-connections>
                    <http-listener>
                        <name>http1</name>
                        <server-listener-ref>http-server</server-listener-ref>
                        <thread-pool>
                            <min>10</min>
                            <max>20</max>
                        </thread-pool>
                    </http-listener>
                </web-connections>
                <session-config>
                    <timeout>30</timeout>
                    <shared>false</shared>
                    <reload-persistent>false</reload-persistent>
                    <tracking-mode>
                        <cookie>true</cookie>
                        <url>false</url>
                        <ssl>false</ssl>
                    </tracking-mode>
                    <session-cookie>
                        <cookie-name>JSESSIONID</cookie-name>
                        <version>0</version>
                        <max-age>-1</max-age>
                        <secure>false</secure>
                        <http-only>true</http-only>
                    </session-cookie>
                </session-config>
            </web-engine>
            <jms-engine>
                <service-config>
                    <name>internal</name>
                    <listener-name>jms-internal</listener-name>
                    <client-limit>1000</client-limit>
                    <client-keepalive-timeout>20</client-keepalive-timeout>
                </service-config>
                <thread-pool>
                    <min>10</min>
                    <max>20</max>
                </thread-pool>
                <connection-factory>
                    <type>queue</type>
                    <name>QueueConnectionFactory</name>
                </connection-factory>
                <connection-factory>
                    <type>topic</type>
                    <name>TopicConnectionFactory</name>
                </connection-factory>
            </jms-engine>
            <jms-resource>
                <destination>
                    <type>queue</type>
                    <name>ExamplesQueue</name>
                </destination>
                <destination>
                    <type>topic</type>
                    <name>ExamplesTopic</name>
                </destination>
            </jms-resource>
            <system-logging>
                <name>jeus</name>
                <level>INFO</level>
                <handler>
                    <file-handler>
                        <name>fileHandler</name>
                        <valid-day>1</valid-day>
                    </file-handler>
                </handler>
            </system-logging>
        </server>
        <server>
            <name>server1</name>
            <node-name>2269097c89bc</node-name>
            <jvm-config>
                <jvm-option>-Xmx1024m -XX:MaxMetaspaceSize=128m</jvm-option>
