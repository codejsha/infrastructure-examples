                    <name>http-server1</name>
                    <listen-port>8088</listen-port>
                </listener>
                <listener>
                    <name>jms-internal1</name>
                    <listen-port>9741</listen-port>
                </listener>
            </listeners>
            <system-logging>
                <name>jeus</name>
                <level>INFO</level>
                <handler>
                    <file-handler>
                        <name>fileHandler</name>
                        <valid-day>1</valid-day>
                    </file-handler>
                </handler>
            </system-logging>
            <web-engine>
                <blocked-url-patterns>
                    <deny-last-space-character>true</deny-last-space-character>
                    <deny-null-character>true</deny-null-character>
                    <encoded-pattern>%23</encoded-pattern>
                    <encoded-pattern>%2e</encoded-pattern>
                    <encoded-pattern>%2f</encoded-pattern>
                    <encoded-pattern>%5c</encoded-pattern>
                    <encoded-pattern>%00</encoded-pattern>
                    <decoded-pattern>#</decoded-pattern>
                </blocked-url-patterns>
                <web-connections>
                    <http-listener>
                        <name>http1</name>
                        <server-listener-ref>http-server1</server-listener-ref>
                        <thread-pool>
                            <min>10</min>
                            <max>20</max>
                        </thread-pool>
                    </http-listener>
                </web-connections>
                <session-config>
                    <timeout>30</timeout>
                    <shared>false</shared>
                    <reload-persistent>false</reload-persistent>
                    <tracking-mode>
                        <cookie>true</cookie>
                        <url>false</url>
